#!/bin/bash
#set -e

#================================================================================
#================================================================================
# Generate the ensemble

module purge
module use -a /scratch2/NCEPDEV/marine/marineda/modulefiles/
module load jedi-intel-17.0.5.239

echo $RUNCDATE

if [ ! -d $RUNCDATE ]; then mkdir -p $RUNCDATE; fi
cd $RUNCDATE

# Generate the ensemble using soca_enspert

srun -n $npe_gen ${SOCA_EXEC}/soca_enspert.x ./yaml/genenspert.yml 
